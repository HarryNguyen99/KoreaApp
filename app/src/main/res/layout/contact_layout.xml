<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/img_bg_contact_1" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/img_bg_contact_2" />

    <TextView
        android:id="@+id/tv_tieu_de"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#FF8F07"
        android:fontFamily="@font/roboto_bold"
        android:gravity="center"
        android:text="Số điện thoại khẩn cấp"
        android:textColor="#000"
        android:textSize="25sp" />

    <LinearLayout
        android:id="@+id/ll_tim_kiem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_tieu_de"
        android:background="#393e42">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/gry_round_tk"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="10dp">

            <ImageView
                android:id="@+id/img_tim_kiem"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:padding="5dp"
                android:src="@mipmap/ic_tim_kiem"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"/>

            <EditText
                android:id="@+id/edt_tim_kiem"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginRight="10dp"
                android:hint="Tìm kiếm"
                android:textColor="#8692a0"
                android:textSize="20sp"
                android:textColorHint="#8692a0"
                android:layout_toRightOf="@+id/img_tim_kiem"
                android:background="@null"/>

        </RelativeLayout>



    </LinearLayout>


    <View
        android:id="@+id/v_tim_kiem"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@+id/ll_tim_kiem"
        android:background="#000"></View>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/v_tim_kiem">

        <ListView
            android:id="@+id/lv_sdt_khan_cap"
            android:layout_width="match_parent"
